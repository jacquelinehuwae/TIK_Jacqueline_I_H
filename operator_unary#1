#include <iostream>
using namespace std;

int main(){
    //Operator Unary
    int a = 10;
    a = a++; //increment, menambahkan 1 nilai kedalam var
    //var a akan ditampilkan baru ditambahkan kemudian.
    
    
    int b = 10;
    b = ++b; //increment , menambahkan 1 nilai kedalam var
    //var b 1+ 10 langsung ditampilkan
  
  //decrement
  int c = 10;
  c= c--; //pre-decrement 
  
  int d = 10;
  d = --d; //post-decrement
  
    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    cout<<d<<endl;
    
    
    return 0;
}
